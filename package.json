{
  "name": "gafemoyano11ty",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean --parallel eleventy css",
    "dev": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "npx @11ty/eleventy --serve",
    "css": "npx @tailwindcss/cli -i src/assets/styles/tailwind.css -o _site/assets/styles/tailwind.css -w",
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open",
    "build": "cross-env ELEVENTY_ENV=production npx @11ty/eleventy & npx @tailwindcss/cli -i src/assets/styles/tailwind.css -o _site/assets/styles/tailwind.css --minify",
    "clean": "npx rimraf _site"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@tailwindcss/postcss": "^4.0.0",
    "auto-prefixer": "^0.4.2",
    "autoprefixer": "^10.4.22",
    "browser-sync": "^3.0.4",
    "cross-env": "^7.0.2",
    "eleventy-plugin-i18n": "^0.1.3",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.1",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-footnote": "^3.0.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^12.0.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^4.0.0"
  },
  "engines": {
    "node": "18.x"
  }
}
